CREATE TABLE IF NOT EXISTS G_CHE_ASSO (
    ASSO_ID INT AUTO_INCREMENT,
    CHEMO_DRG VARCHAR(255) NOT NULL,
    GENE VARCHAR(255) NOT NULL,
    STATUS VARCHAR(255),
    ASSO_TYPE VARCHAR(255) NOT NULL,
    GENE_CHE_CONTRI FLOAT(5,3) NOT NULL,
    PRIMARY KEY (ASSO_ID)
)  ENGINE=INNODB;

CREATE TABLE IF NOT EXISTS P_CHE_ASSO (
    ASSO_ID INT AUTO_INCREMENT,
    CHEMO_DRG VARCHAR(255) NOT NULL,
    PATHWAY VARCHAR(255) NOT NULL,
    STATUS VARCHAR(255),
    ASSO_TYPE VARCHAR(255) NOT NULL,
    GENE_CHE_CONTRI FLOAT(5,3) NOT NULL,
    PRIMARY KEY (ASSO_ID)
)  ENGINE=INNODB;

CREATE TABLE IF NOT EXISTS G_HCOMP_ASSO (
    ASSO_ID INT AUTO_INCREMENT,
    HCOMP VARCHAR(255) NOT NULL,
    GENE VARCHAR(255) NOT NULL,
    TARGET_TYPE VARCHAR(255) NOT NULL,
    TARGET_ACTION VARCHAR(255) NOT NULL,
    TARGET_RANK FLOAT(4,1) NOT NULL,
    PRIMARY KEY (ASSO_ID)
)  ENGINE=INNODB;

CREATE TABLE IF NOT EXISTS P_HCOMP_ASSO (
    ASSO_ID INT AUTO_INCREMENT,
    HCOMP VARCHAR(255) NOT NULL,
    PATHWAY VARCHAR(255) NOT NULL,
    TARGET_TYPE VARCHAR(255) NOT NULL,
    TARGET_ACTION VARCHAR(255) NOT NULL,
    TARGET_RANK FLOAT(4,1) NOT NULL,
    PRIMARY KEY (ASSO_ID)
)  ENGINE=INNODB;

CREATE TABLE IF NOT EXISTS INDC_COMPOSITION (
    INDC_ID INT AUTO_INCREMENT,
    INDC_NAME VARCHAR(255) NOT NULL,
    GENE VARCHAR(255) NOT NULL,
    MUT_FREQ FLOAT(5,3) NOT NULL,
    CNV_UP_FREQ FLOAT(5,3) NOT NULL,
    CNV_DOWN_FREQ FLOAT(5,3) NOT NULL,
    PRIMARY KEY (INDC_ID)
)  ENGINE=INNODB;

CREATE TABLE IF NOT EXISTS GP_MAP (
    ID INT AUTO_INCREMENT,
    GENE VARCHAR(255) NOT NULL,
    PATHWAY VARCHAR(255) NOT NULL,
    WEIGHT FLOAT(5,3) NOT NULL,
    PRIMARY KEY (ID)
)  ENGINE=INNODB;

CREATE TABLE IF NOT EXISTS LOG_INPUT (
    ENTRY_ID INT AUTO_INCREMENT,
    UNIT_ID VARCHAR(255) NOT NULL,
    INDC VARCHAR(255) NOT NULL,
    GENE VARCHAR(255),
    ABR_TYPE VARCHAR(255),
    ABR_EFF VARCHAR(225),
    PATHWAY VARCHAR(255),
    PATH_VALUE VARCHAR(255),
    PRIMARY KEY (ENTRY_ID)
)  ENGINE=INNODB;
